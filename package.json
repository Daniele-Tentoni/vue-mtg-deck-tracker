{
  "name": "vue-mtg-deck-tracker",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "prepare": "cypress install",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "supabase:types": "npx supabase gen types typescript --project-id awvhzmqrqxrqlsohpsqr > src/types/database.types.ts",
    "update-types": "run-s supabase:types format"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.95.3",
    "blueimp-md5": "^2.19.0",
    "pinia": "^3.0.4",
    "vue": "^3.5.28",
    "vue-router": "^5.0.2",
    "vuetify": "^3.11.8"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^7.2.0",
    "@tsconfig/node24": "^24.0.4",
    "@types/blueimp-md5": "^2.18.2",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.13",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/eslint-plugin": "^1.6.6",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "cypress": "^15.10.0",
    "eslint": "^9.39.2",
    "eslint-plugin-cypress": "^5.3.0",
    "eslint-plugin-vue": "~10.8.0",
    "jiti": "^2.6.1",
    "jsdom": "^28.1.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.8.1",
    "start-server-and-test": "^2.1.3",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-vue-devtools": "^8.0.6",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.2"
  }
}
